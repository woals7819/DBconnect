package baord;

import java.util.ArrayList;

public class Test {

	public static void main(String[] args) {
		ArrayList alist = new ArrayList();

		alist.add(5);
		alist.add(4);
		alist.add(3);
		alist.add(2);
		alist.add(1);
		alist.add(0);

		int size = alist.size();
		System.out.println(size);
		System.out.println(alist.get(2));

	}

}
