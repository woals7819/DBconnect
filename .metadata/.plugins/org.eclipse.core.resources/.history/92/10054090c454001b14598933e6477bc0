package day01;

import java.util.Scanner;

public class gugu {

	public static void main(String[] args) {
		System.out.println("원하는 단을 입력해주세요 :");
		 Scanner sc = new Scanner(System.in);
		 int num = sc.nextInt();
		 
		 for(int i = 1; i < 10, i++) {
			 System.out.println(num + "*" + i + "=" + num*i);
		 }		 

	}

}
